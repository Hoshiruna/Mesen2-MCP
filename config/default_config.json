{
    "dll_path": null,
    "auto_initialize_debugger": true,
    "streaming": {
        "enabled": false,
        "polling_rate_hz": 10,
        "max_queue_size": 1000,
        "rate_limits": {
            "max_trace_lines_per_second": 1000,
            "max_events_per_second": 100,
            "max_memory_changes_per_second": 50
        },
        "backoff": {
            "enabled": true,
            "min_delay_seconds": 0.1,
            "max_delay_seconds": 1.0,
            "multiplier": 2.0
        }
    },
    "tools": {
        "max_memory_read_size": 4096,
        "max_trace_lines": 1000,
        "max_disassembly_lines": 100,
        "max_debug_events": 1000,
        "default_cpu_type": "auto"
    },
    "thread_safety": {
        "use_locks": true,
        "lock_timeout_seconds": 5.0,
        "max_concurrent_calls": 10
    },
    "dll_lifecycle": {
        "check_dll_on_startup": true,
        "auto_reconnect_on_failure": false,
        "initialization_timeout_seconds": 1.0,
        "health_check_interval_seconds": 30.0
    },
    "logging": {
        "level": "INFO",
        "file": null,
        "console": true,
        "format": "[%(levelname)s] %(message)s"
    },
    "performance": {
        "cache_memory_size": false,
        "batch_memory_operations": true,
        "prefetch_trace_lines": false
    }
}
